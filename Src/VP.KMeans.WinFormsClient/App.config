<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="TextFileListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="D:\KMeansLog.txt"/>
      </listeners>
    </trace>
    <switches>
      <add name="ErrSev" value="0"/>
      <!--
      0: Off
      1: Error
      2: Warnings
      3: Info
      4: Verbose
      -->
    </switches>
  </system.diagnostics>

</configuration>